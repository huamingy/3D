import request from '@/utils/request'

export function selectEnergyLeftData() {
  return request({
    url: '/modbus/energy/selectEnergyLeftData',
    method: 'get',
  })
}

export function selectEnergyCenterData() {
  return request({
    url: '/modbus/energy/selectEnergyCenterData',
    method: 'get',
  })
}

export function selectEnergyRightData() {
  return request({
    url: '/modbus/energy/selectEnergyRightData',
    method: 'get',
  })
}