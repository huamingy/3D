      function moveup(mesh,num) {
        var pos = mesh.getWorldPosition()
        var tween = new TWEEN.Tween(pos)
        console.log("运行");
        tween.to(
          {
            x: pos.x=num,
          },
          3000
        )
        tween.onUpdate(function () {
          mesh.position.x = pos.x
        })
        tween.start()
      }
